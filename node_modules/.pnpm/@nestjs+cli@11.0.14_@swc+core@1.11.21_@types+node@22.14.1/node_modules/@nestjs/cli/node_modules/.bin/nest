#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mamo/Projects/test/gre1/node_modules/.pnpm/@nestjs+cli@11.0.14_@swc+core@1.11.21_@types+node@22.14.1/node_modules/@nestjs/cli/bin/node_modules:/home/mamo/Projects/test/gre1/node_modules/.pnpm/@nestjs+cli@11.0.14_@swc+core@1.11.21_@types+node@22.14.1/node_modules/@nestjs/cli/node_modules:/home/mamo/Projects/test/gre1/node_modules/.pnpm/@nestjs+cli@11.0.14_@swc+core@1.11.21_@types+node@22.14.1/node_modules/@nestjs/node_modules:/home/mamo/Projects/test/gre1/node_modules/.pnpm/@nestjs+cli@11.0.14_@swc+core@1.11.21_@types+node@22.14.1/node_modules:/home/mamo/Projects/test/gre1/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mamo/Projects/test/gre1/node_modules/.pnpm/@nestjs+cli@11.0.14_@swc+core@1.11.21_@types+node@22.14.1/node_modules/@nestjs/cli/bin/node_modules:/home/mamo/Projects/test/gre1/node_modules/.pnpm/@nestjs+cli@11.0.14_@swc+core@1.11.21_@types+node@22.14.1/node_modules/@nestjs/cli/node_modules:/home/mamo/Projects/test/gre1/node_modules/.pnpm/@nestjs+cli@11.0.14_@swc+core@1.11.21_@types+node@22.14.1/node_modules/@nestjs/node_modules:/home/mamo/Projects/test/gre1/node_modules/.pnpm/@nestjs+cli@11.0.14_@swc+core@1.11.21_@types+node@22.14.1/node_modules:/home/mamo/Projects/test/gre1/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nest.js" "$@"
else
  exec node  "$basedir/../../bin/nest.js" "$@"
fi
